<Window x:Class="ProductivityTools.Meetings.WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProductivityTools.Meetings.WPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">

    <Grid Margin="10">
        <StackPanel Orientation="Horizontal" Height="35" VerticalAlignment="Top">
            <Button Content="Meeting List" Click="Button_Click"></Button>
            <Button Content="New meeting" Click="Button_Click_1"></Button>
        </StackPanel>
        <Frame Margin="0,35,0,0" Name="Frame" Source="{Binding Path=ActivePage}"></Frame>
    </Grid>

</Window>
